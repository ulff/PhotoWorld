{% trans_default_domain 'FOSUserBundle' %}

<div class="center-container">
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-custom">

        {{ form_widget(form) }}

        <div>
            <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-success btn-lg" />
            <a class="btn btn-warning" href="{{ path('UlffPhotoWorldBundle_homepage') }}">Cancel</a>
        </div>
    </form>
</div>
